{
  "name": "stlato-ui-theme",
  "version": "1.0.6",
  "sideEffects": false,
  "license": "MIT",
  "author": "Stlato",
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stlato/stlato-ui.git"
  },
  "keywords": [
    "design-tokens",
    "theme"
  ],
  "bugs": {
    "url": "https://github.com/stlato/stlato-theme/issues"
  },
  "homepage": "https://github.com/stlato/stlato-theme#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build:sd": "style-dictionary build --config ./sd-config.json",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.tx --format esm,cjs --watch --dts",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-stlato": "^1.1.0",
    "style-dictionary": "^3.7.0",
    "tsup": "^6.7.0",
    "typescript": "~5.0.4"
  }
}
